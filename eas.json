{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "NODE_ENV": "production",
        "VERSION_CODE": "16"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "node": "22.19.0",
      "autoIncrement": true,
      "channel": "production",
      "env":{
        "NPM_CONFIG_PRODUCTION": "false",
        "npm_config_production": "false"
      },
      "cache": { "disabled": true }
    }
  },
  "submit": {
    "production": {}
  }
}